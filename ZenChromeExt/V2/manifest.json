{
  "manifest_version": 2,

  "name": "ZenExt Web Blocker",
  "description": "This extension blocks internet",
  "version": "1.0",
  "permissions": [
                "http://*/*",
            	"https://*/*"
  ],
  "optional_permissions": [
             	"webRequest",
             	"webRequestBlocking"
  ],
  "browser_action": {
    "default_icon": "ok.png",
    "default_popup": "popup.html"
  }
}